{
  "source": {
    "include": ["index.js", "src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "docdash": {
      "static": true,
      "sort": true,
      "collapse": false,
      "typedefs": true,
      "removeQuotes": "none"
    }
  }
}
